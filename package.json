{
  "name": "Ionic",
  "version": "1.0.0",
  "description": "Ionic Webpack starter",
  "scripts": {
    "build:dev": "rimraf dist && webpack -d --progress --config config/webpack.dev.js",
    "build:cordova": "rimraf cordova/www && webpack -p --progress --config config/webpack.prod.js",
    "start": "webpack-dev-server -d --progress --config config/webpack.dev.js",
    "test": "karma start config/karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor config/protractor.conf.js",
    "tslint": "tslint -p tsconfig.json -t verbose ./src/**/*.ts"
  },
  "author": "Iulian Stan",
  "license": "ISC",
  "dependencies": {
    "@angular-devkit/architect": "^0.802.0",
    "@angular-devkit/build-angular": "^0.802.0",
    "@angular/common": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/forms": "^8.2.0",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/router": "^8.2.0",
    "@ionic-native/core": "^5.11.0",
    "@ionic-native/splash-screen": "^5.11.0",
    "@ionic-native/status-bar": "^5.11.0",
    "@ionic/angular": "^4.7.1",
    "@ionic/angular-toolkit": "^2.0.0",
    "core-js": "^2.6.9",
    "rxjs": "^6.3.1",
    "rxjs-compat": "^6.2.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/compiler": "^8.2.0",
    "@angular/compiler-cli": "^8.2.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/webpack-env": "^1.13.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0",
    "node-sass": "^4.11.0",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "to-string-loader": "^1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
